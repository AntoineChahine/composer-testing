{
  "name": "teelaunch/2.0-api-laravel-sdk",
  "version": "1.0.0",
  "description": "A wrapper for teelaunch's REST API",
  "type": "package",
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^8.0|^9.0",
    "ext-curl": "*",
    "ext-json": "*",
    "monolog/monolog": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "TeeLaunch\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "laravel/pint": "^1.13"
  },
  "repositories": {
    "packagist.org": false,
    "private-packagist": {
      "type": "composer",
      "url": "https://repo.packagist.com/testing/"
    }
  }
}
